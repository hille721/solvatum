  keywords = {Molecular Interactioons, Solutions, Thermodynamics},
  owner    = {deimel},
  url      = {https://onlinelibrary.wiley.com/doi/abs/10.1002/bbpc.19971010810},
}

@Article{Mokbel1998,
  author   = {I. Mokbel and A. Blondel-Telouk and D. Vellut and J. Jose},
  title    = {Vapor--liquid equilibria of two binary mixtures: benzene+n-tetradecane and benzene+squalane},
  journal  = {Fluid Phase Equilib.},
  year     = {1998},
  volume   = {149},
  number   = {1},
  pages    = {287 - 308},
  issn     = {0378-3812},
  abstract = {The authors have modified a tonometer previously used for average molecular weights determination of complex mixtures in order to measure vapor–liquid equilibria of pure compounds and mixtures. Two binary systems with components of different volatility have been studied within the pressure range 1332≤P (Pa)≤200 000 and mole fraction range: 0≤x≤1. The two binary systems studied are benzene+n-tetradecane and benzene+squalane. The experimental data (xi, Ptotal) have been reduced by Barker's method using a Redlich–Kister expansion in order to evaluate the excess Gibbs energy GE and the vapor phase composition yi. The excess enthalpy HE has been estimated from the GE values. For the binary mixture benzene+n-C14, our experimental results are in good agreement with literature data (relative error ≤2% for P and HE). Excess volumes of the two binary systems are also reported.},
  doi      = {https://doi.org/10.1016/S0378-3812(98)00271-4},
  keywords = {Experimental data, Hydrocarbons, Low vapor pressure, Excess Gibbs energy, Excess enthalpy, Excess volume},
  owner    = {deimel},
  url      = {http://www.sciencedirect.com/science/article/pii/S0378381298002714},
}

