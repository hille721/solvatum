@Article{Lepori2015,
  author   = {L. Lepori and E. Matteoli and P. Gianni and M. C. Righetti},
  title    = {Thermodynamic study of heptane+amine mixtures. V. Excess and solvation Gibbs energies},
  journal  = {Fluid Phase Equilib.},
  year     = {2015},
  volume   = {387},
  pages    = {198 - 208},
  issn     = {0378-3812},
  abstract = {Vapor–liquid equilibria of binary mixtures of n-heptane with 15 amines, primary n-alkyl (C3–C8), secondary (diethyl to dibutyl), branched (iso-propyl and sec-butyl) and cyclic amines (piperidine, hexamethyleneimine, heptamethyleneimine and cyclopentylamine) have been determined by head-space gas chromatographic analysis of the vapor phase directly withdrawn from an equilibration apparatus. Excess molar Gibbs energies and activity coefficients for the systems investigated have been obtained by a least-square treatment of the equilibrium results. All mixtures exhibit positive deviations from ideality, decreasing with the amine size. The Gibbs energy of solvation for amines in heptane has been evaluated and its dependence on polar groups, chain lengthening, branching and cyclization has been discussed.},
  doi      = {https://doi.org/10.1016/j.fluid.2014.12.017},
  keywords = {Vapour–liquid equilibrium, Excess Gibbs energy, Activity coefficient, Gibbs energy of solvation, Amines, Heptane, Group contribution},
  owner    = {deimel},
  url      = {http://www.sciencedirect.com/science/article/pii/S0378381214006980},
}

@Article{Lepori2016,
  author   = {Lepori, Luciano and Gianni, Paolo and Matteoli, Enrico},
  title    = {Thermodynamic study of tetrachloromethane or heptane + cycloalkane mixtures},
  journal  = {J. Therm. Anal. Calorim.},
  year     = {2016},
  volume   = {124},
  number   = {3},
  pages    = {1497-1509},
  month    = {Jun},
  issn     = {1588-2926},
  abstract = {Vapor--liquid equilibria at 298.15 K have been determined for binary mixtures of tetrachloromethane or heptane plus a cyclic hydrocarbon (cyclopentane, cyclohexane, cycloheptane, cyclooctane, cyclodecane, cis-decalin, norbornane, octahydro-4,7-methano-1H-indene, adamantane), using a head-space gas chromatographic technique. Excess molar Gibbs energies and activity coefficients for the systems investigated have been obtained by a least-squares treatment of the equilibrium results. All mixtures exhibit positive deviations from ideality, increasing with the cycloalkane size. The Gibbs energies of solvation, $\Delta$solv                        G{\textdegree}, for cyclic hydrocarbons in the two solvents have been evaluated and compared with those of linear and branched alkanes. The effect of cyclization upon $\Delta$solv                        G{\textdegree} has been discussed.},
  day      = {01},
  doi      = {10.1007/s10973-016-5326-9},
  owner    = {deimel},
  url      = {https://doi.org/10.1007/s10973-016-5326-9},
}

@Article{Li1999,
  author   = {Li, Jiabo and Zhu, Tianhai and Hawkins, Gregory D. and Winget, Paul and Liotard, Daniel A. and Cramer, Christopher J. and Truhlar, Donald G.},
  title    = {Extension of the platform of applicability of the SM5.42R universal solvation model},
  journal  = {Theor. Chem. Acc.},
  year     = {1999},
  volume   = {103},
  number   = {1},
  pages    = {9-63},
  month    = {Nov},
  issn     = {1432-2234},
  abstract = {We present eight new parameterizations of the SM5.42R solvation model: in particular we present parameterizations for HF/MIDI!, HF/6-31G*, HF/6-31+G*, HF/cc-pVDZ, AM1, PM3, BPW91/MIDI!, and B3LYP/MIDI!. Two of the new cases are parameterized using the reaction-field operator presented previously, and six of the new cases are parameterized with a simplified reaction-field operator; results obtained by the two methods are compared for selected examples. For a training set of 2135 data for 275 neutral solutes containing H, C, N, O, F, S, P, Cl, Br, and I in 91 solvents (water and 90 nonaqueous solvents), seven of the eight new parameterizations give mean unsigned errors in the range 0.43--0.46{\thinspace}kcal/mol, and the eighth -- for a basis set containing diffuse functions -- gives a mean unsigned error of 0.53{\thinspace}kcal/mol. The mean unsigned error for 49 ionic solutes (containing the same elements) in water is 3.5--3.9{\thinspace}kcal/mol for the Hartree--Fock, Becke--Perdew--Wang-1991 and Becke three-parameter Lee--Yang--Parr cases and 4.1 and 4.0{\thinspace}kcal/mol for parameterized model 3 and Austin model 1, respectively. The methods are tested for sensitivity of solvation free energies to geometry and for predicting partition coefficients of carbonates, which were not included in the training set.},
  day      = {01},
  doi      = {10.1007/s002140050513},
  owner    = {deimel},
  url      = {https://doi.org/10.1007/s002140050513},
}

@Article{Liang2018,
  author   = {Xiaojiang Liang and Shilin Ye and Qinglong Xie and Meizhen Lu and Fan Xia and Yong Nie and Zhiyan Pan and Jianbing Ji},
  title    = {Solubilities of sulfuryl fluoride in propylene carbonate, tributyl phosphate and N-methylpyrrolidone},
  journal  = {J. Chem. Thermodyn.},
  year     = {2018},
  volume   = {125},
  pages    = {11 - 16},
  issn     = {0021-9614},
  abstract = {The solubilities of sulfuryl fluoride (SO2F2) in propylene carbonate (PC), tributyl phosphate (TBP) and N-methylpyrrolidone (NMP) was measured by isochoric saturation method at the pressure of up to 600 kPa and temperatures ranging from (293.15 to 313.15) K. Results showed that SO2F2 solubility increased with increasing pressure and decreased with increasing temperature in the three solvents, and the dissolution of SO2F2 in the solvents was a physical process. Moreover, Henry’s law constants and thermodynamic properties such as standard Gibbs free energy, enthalpy, and entropy changes of SO2F2 solvation were further calculated according to the experimental solubility data. The solubility of SO2F2 in these three solvents followed the order of TBP > PC > NMP. The results illustrated that TBP possessed the potentiality to capture SO2F2 among the present absorbents due to the relatively large solubility and low absorption enthalpy for SO2F2.},
  doi      = {https://doi.org/10.1016/j.jct.2018.05.007},
  keywords = {Sulfuryl fluoride, Solubility, Henry’s law constant, Thermodynamic properties},
  owner    = {deimel},
  url      = {http://www.sciencedirect.com/science/article/pii/S0021961418303471},
}

